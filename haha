    end)
    end
end)

local extra = window1.new({
    text = "extra",
})



local walking = extra.new("slider", {
    text = "walkspeed",
    color = Color3.new(0.8, 0.5, 0),
    min = 16,
    max = 10000,
    value = 16,
    rounding = 1,
})
walking.event:Connect(function(x)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = x 
end)

local jumping = extra.new("slider", {
    text = "jump power",
    color = Color3.new(0.8, 0.5, 0),
    min = 16,
    max = 10000,
    value = 16,
    rounding = 1,
})
walking.event:Connect(function(x)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = x 
end)

local superhuman = extra.new("switch", {
    text = "super human";
})

superhuman.event:Connect(function(bool)
    supaar = bool 
    if supaar then 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 150 
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 350
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

local walls = extra.new("switch", {
    text = "noclip";
})

walls.event:Connect(function(bool)
    wallie = bool 
    if wallie == true then 
        noclip()
    elseif wallie == false then 
        noclip()
    end
    end
end)
