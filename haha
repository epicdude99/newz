local enginef = loadstring(game:HttpGet("https://raw.githubusercontent.com/Singularity5490/rbimgui-2/main/rbimgui-2.lua"))()

local window1 = enginef.new({
    text = "wayne.lua | a dusty trip",
    size = UDim2.new(300, 400),
})

window1.open()

local main = window1.new({
    text = "main",
})

main.new("label", {
    text = "this script was made by samet.exe",
    color = Color3.new(1, 0, 0),
})

local discord = main.new("button", {
    text = "join discord server",
})

main.new("label", {
    text = "this scripts consists such things as esp and other",
    color = Color3.new(1,0,1),
})

main.new("label", {
    text = "DISABLE AND RE-ENABLE THE TOGGLES TO UPDATE THE ESP",
    color = Color3.new(0.984314, 1.000000, 0.000000),
})

discord.event:Connect(function()
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "wayne.lua", -- Required
        Text = "copied link into clipboard", -- Required
        Duration = 5
    })
    setclipboard("https://discord.gg/fngDKXhJW2")
end)

local gascan = main.new("switch", {
    text = "gas can esp";
})

gascan.event:Connect(function(bool)
    gas = bool
    if gas then 
        gasesp()
        game.Workspace.ChildAdded:Connect(function()
            gasesp()
        end)
    end
end)

local enginon = main.new("switch", {
    text = "engine esp";
})

enginon.event:Connect(function(bool)
    engz = bool 
    if engz then 
    enginesp()
    game.Workspace.ChildAdded:Connect(function()
        engineesp()
    end)
    end
end)

local radion = main.new("switch", {
    text = "radiator esp";
