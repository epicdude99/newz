function mobesp()
    for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
        if v.Name == "mutant" or v.Name == "vampire" or v.Name == "Vampire" or v.Name == "Mutant" then 
            local partgui = Instance.new("BillboardGui")
            partgui.Parent = v.HumanoidRootPart
            partgui.AlwaysOnTop = true 
            partgui.Name = "itemespz"
            partgui.Size = UDim2.new(1,0,1,0)

            local frame = Instance.new("Frame")
            frame.Parent = partgui
            frame.BackgroundColor3 = Color3.fromRGB(255,80,65)
            frame.BackgroundTransparency = 0.25
            frame.Size = UDim2.new(1,0,1,0)
            frame.BorderSizePixel = 0
