-- a dusty trip

local settings = {
    gas_esp = nil,
    engine_esp = nil,
    radiator_esp = nil,
    wheel_esp = nil,
    food_esp = nil,
    weapon_esp = nil,
    mob_esp = nil,
}

local Noclip = nil
local Clip = nil

function noclip()
	Clip = false
	local function Nocl()
		if Clip == false and game.Players.LocalPlayer.Character ~= nil then
			for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
				if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
					v.CanCollide = false
				end
			end
		end
		wait(0.21) -- basic optimization
	end
	Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
	if Noclip then Noclip:Disconnect() end
	Clip = true
end


function gasesp()
        for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
        if v.Name == "GasCan" or v.Name == "gascan" or v.Name == "oilcan" or v.Name == "OilCan" or v.Name == "WaterCan" or v.Name == "watercan" or v.Name == "DieselCan" or v.Name == "dieselcan" then 
            local partgui = Instance.new("BillboardGui")
            partgui.Parent = v
            partgui.AlwaysOnTop = true 
            partgui.Name = "itemespz"
            partgui.Size = UDim2.new(1,0,1,0)

            local frame = Instance.new("Frame")
            frame.Parent = partgui
            frame.BackgroundColor3 = Color3.fromRGB(255,255,255)
            frame.BackgroundTransparency = 0.25
            frame.Size = UDim2.new(1,0,1,0)
            frame.BorderSizePixel = 0
            local namegui = Instance.new("BillboardGui")
            namegui.Parent = v
            namegui.AlwaysOnTop = true 
            namegui.Name = "Name"
            namegui.Size = UDim2.new(1,0,1,0)
            namegui.SizeOffset = Vector2.new(0,1)
            local text = Instance.new("TextLabel")
            text.Parent = namegui
            text.Text = v.Name
            text.TextColor3 = Color3.fromRGB(255,255,255)
            text.TextTransparency = 0.25
            text.BackgroundTransparency = 1
            text.TextScaled = true 
            text.Size = UDim2.new(1,0,1,0)
            text.Font = Enum.Font.Michroma
            text.Name = "Gayy"
        end
    end
end

